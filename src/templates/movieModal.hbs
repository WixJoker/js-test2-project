<div class='movieModalContainer'>

  <div class='movieModalContainerPoster'>
    {{#if poster_path}}
    <img src='https://www.themoviedb.org/t/p/w220_and_h330_face/{{poster_path}}' alt='{{title}}'
      class='movieModalContainerPosterIMG' />
    {{else}}
    <img
      src="data:image/jpeg;base64,"
      alt='{{title}}' class="movieModalContainerPosterIMG">
    {{/if}}
  </div>

  <div class='movieModalContainerData'>
    <h1 class='movieModalContainerDataH1'>{{title}}</h1>
    <table class="movieModalContainerDataStat">
      <tr class='statrow'>
        <td class='movieModalContainerDataStatItemName'>Vote / Votes</td>
        <td class='movieModalContainerDataStatItemResult'><span class='movieVotes'>{{vote_average}}</span> / <span
            class='movieVotes'>{{vote_count}}</span></td>
      </tr>
      <tr class='statrow'>
        <td class='movieModalContainerDataStatItemName'>Popularity</td>
        <td class='movieModalContainerDataStatItemResult'>{{popularity}}</td>
      </tr>
      <tr class='statrow'>
        <td class='movieModalContainerDataStatItemName'>Original Title</td>
        <td class='movieModalContainerDataStatItemResult'>{{original_title}}</td>
      </tr>
      <tr class='statrow'>
        <td class='movieModalContainerDataStatItemName'>Genre</td>
        <td class='movieModalContainerDataStatItemResult'>{{#each genres}} {{this.name}} {{/each}}</td>
      </tr>
    </table>
    <p class='movieModalContainerDataAboutHeader'>ABOUT</p>
    <p class='movieModalContainerDataAbout'>{{overview}}</p>
<div>
  <button type="button" class="btn btn-primary movieModalContainerDataBtn" data-action='toWatch' data-id='{{id}}'>ADD
    TO WATCH</button>
  <button type="button" class="btn btn-primary movieModalContainerDataBtn" data-action='toQueue' data-id='{{id}}'>ADD
    TO QUEUE</button>
</div>
      <span class='movie-modal-span'></span>
  </div>
</div>
